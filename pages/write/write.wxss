/* incomplete/incomplete.wxss */
.write {
  display: flex;
  flex-direction: column;
}

.title {
  padding-top: 20rpx;
  padding-bottom: 20rpx;
  height: 80rpx !important;
  margin: auto;
  font-size: 38rpx !important;
  width: 90%;
  text-align: left;
  display: column;
}

.division {
  height: 2rpx;
  width: 90%;
  background-color: #ceced1;
  margin: auto;
}

.content {
  padding-top: 20rpx;
  padding-bottom: 20rpx;
  margin: auto;
  font-size: 30rpx;
  width: 90%;
  text-align: left;
  display: column;
}

.chooseImg {

  background-color: #ffffff;
  width: 220rpx;
  height: 220rpx;
  display: flex;
  align-items: center;
  vertical-align: center;
  justify-content: center;
  flex-direction: row;
  box-shadow: 0rpx 0rpx 26rpx 2rpx #ccc;
}

.chooseImg-content {
  display: flex;
  flex-direction: column;
}

.chooseImg-text {
  color: #868690;
  font-size: 26rpx;
  padding-top: 15rpx;
  text-align: center;
}

.svg {
  background: url("data:image/svg+xml;base64,");
  width: 80rpx;
  height: 80rpx;
  background-size: contain;

  padding: auto;
}

button {
  border: 0 solid #0010ae;
  background-color: #1f2afe;
  color: #fff;
  font-size: 120%;
  padding: 8px 16px;
  outline-width: 0;
  -webkit-appearance: none;
  box-shadow: 0 8px 17px rgba(0, 0, 0, 0.2);
}

.commit {
  display: block;
  margin: 24px auto;
}




.title {
  padding-left: 31rpx;
  font-size: 28rpx;
  font-weight: bold;
  color: #333333;
}

.img_list {
  flex-wrap: wrap;
}

.item_img {
  width: 220rpx;
  height: 220rpx;
  margin-right: 23rpx;
  margin-bottom: 23rpx;
  position: relative;
  /* border: 1px solid #ccc; */
}

.img_item {
  width: 220rpx;
  height: 220rpx;
}

.item_img:nth-child(3n) {
  margin-right: 0;
}

.chooseImg{
  width: 220rpx;
  height: 220rpx;
  margin-bottom: 23rpx;
  border: 1px solid #ccc;
  justify-content: center;
}



.add_img {
  width: 60rpx;
  height: 60rpx;
  margin-bottom: 30rpx;
}

.close {
  width: 30rpx;
  height: 30rpx;
  position: absolute;
  top: -15rpx;
  right: -15rpx;
}
.flex-row{
  display: flex;
  flex-direction: row;
  align-items: center;
  margin-left: 20rpx;
  margin-right: 20rpx;
}